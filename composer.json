{
    "name": "newnaratif/wp-plugin-reporter",
    "description": "A WordPress plugin to report on installed plugins",
    "type": "wordpress-plugin",
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "yoast/phpunit-polyfills": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "PluginReporter\\": "includes/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
